{
  "expo": {
    "name": "Earn Pilot",
    "slug": "earn-pilot-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/NewPilot.png",
    "userInterfaceStyle": "automatic",
    "scheme": "earnpilot",
    "splash": {
      "image": "./assets/images/NewPilot.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A1F44"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.networks11.earnpilot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/NewPilot.png",
        "backgroundColor": "#0A1F44"
      },
      "package": "com.networks11.earnpilot",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/NewPilot.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/NewPilot.png",
          "color": "#3B82F6"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "156da554-3870-41b3-9a68-2191a71f936d"
      },
      "API_BASE_URL": "http://127.0.0.1:8000"
    }
  }
}
